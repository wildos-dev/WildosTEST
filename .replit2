modules = ["python-3.11", "nodejs-20", "postgresql-16"]

[nix]
channel = "stable-24_05"
packages = ["c-ares", "cacert", "cargo", "glibcLocales", "grpc", "libffi", "libiconv", "libsodium", "libxcrypt", "libyaml", "openssh", "openssl", "pkg-config", "postgresql", "protobuf", "rustc", "sqlite", "sshpass", "unzip", "xcodebuild", "zlib"]

[workflows]
runButton = "Dashboard Dev"

[[workflows.workflow]]
name = "Dashboard Dev"
author = 47505407
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd dashboard && npm run dev"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33523
externalPort = 3000
